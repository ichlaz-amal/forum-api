{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "class-methods-use-this": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": "off",
    "object-curly-newline": "off"
  }
}
